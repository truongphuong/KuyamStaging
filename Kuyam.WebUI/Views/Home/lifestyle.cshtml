<script type="text/javascript">

    $(document).ready(function () {

     $('#thrivenextarrow').click(function () {
         var isLogin = @Request.IsAuthenticated.ToString().ToLower();
            if (!isLogin) {
                $('#loginError').hide();
                showDialog('signuppopup', 'btnCloseloginPopup');
                return false;
            }
            window.location = "/Company/CompanySetup";
    });

     $('select#category').selectmenu();
        $('#password').hide();
        $('#passwordtext').focus(function () {
            $('#passwordtext').hide();
            $('#password').show();
            $('#password').css('color', '#333333');
            $('#password').focus();
        });
        $('#password').blur(function () {
            if ($('#password').val() == '') {
                $('#passwordtext').show();
                $('#password').hide();
            }
        });

        $('#btnlifestylenext').click(function () {
