                    {{offer.Event.Name}}
                    <br />
                    <span>
                        {{offer.Event.StartDate | date:'MMM dd'}} -
                        <span ng-if="offer.Event.EndDate.getMonth() != offer.Event.StartDate.getMonth()">
                            {{offer.Event.EndDate | date:'MMM'}}
                        </span>
                        {{offer.Event.EndDate | date:'dd'}}
                    </span>
