@section scripts{
<script type="text/javascript">
    (function () {
        'uses strict';
        var infoApp = angular.module('CompanyProfileApp', []);
        infoApp.controller("HomeCtrl", ["$scope", "$http", function ($scope, $http) {
            $scope.favorite = @Model.Favorite.ToString().ToLower();
            $scope.isAuthenticated = @Request.IsAuthenticated.ToString().ToLower();
            $scope.offer = {};

            $scope.addToFavorite = function() {
                if ('@(Request.IsAuthenticated?"1":"0")' == '0') {
                    showDialog('signuppopup', 'btnCloseloginPopup');
                    return;
                }
                var valu2 = $('#profileid').val();
                var searchParameters = { profileid: valu2 };
                $.ajax(
                    {
                        type: 'POST',
                        contentType: 'application/json; charset=utf-8',
                        data: JSON.stringify(searchParameters),
                        dataType: 'html',
                        url: '/CompanyProfile/AddToFavorite/'
                    })
                    .success(function (result) {
                        $scope.$apply(function(){
                            $scope.favorite = true;
                        });
                    })
                    .error(function (error) {
                        alert('error');
                    });
            }

            $scope.removeFavorite = function(){
                if ('@(Request.IsAuthenticated?"1":"0")' == '0') {
                    showDialog('signuppopup', 'btnCloseloginPopup');
                    return;
                }
                var valu2 = $('#profileid').val();
                var searchParameters = { profileid: valu2 };
                $.ajax(
                        {
                            type: 'POST',
                            contentType: 'application/json; charset=utf-8',
                            data: JSON.stringify(searchParameters),
                            dataType: 'html',
                            url: '/CompanyProfile/RemoveFromFavorite/'
                        })
                        .success(function (result) {
                            $scope.$apply(function(){
                                $scope.favorite = false;
                            });
                        })
                        .error(function (error) {
                            alert('error');
                        });
            };

            $scope.getOfferInfo = function(offerId){
                $('#modal-offer').modal({'show': true});
                var url = "/company/GetOffer/?companyEventId="+ offerId + "&nocache=" + getunixtime();
                $http({
                    method: 'GET',
                    url: url
                }).
                    success(function(data, status, headers, config){
                        $scope.offer = data;
                        $scope.offer.Event.StartDate = new Date($scope.offer.Event.StartDate);
                        $scope.offer.Event.EndDate = new Date($scope.offer.Event.EndDate);
                    }).
                    error(function(data, status, headers, config){

                    });
            };

            $scope.checkAvaibility = function(serviceId, isClass){
                //alert(serviceId);
                var url = "/availability" + $scope.offer.SlugName + "?serviceId=" + serviceId;
                if(isClass){
                    url = "/class" + $scope.offer.SlugName + "?serviceId=" + serviceId;
                }
                window.location.href = url;
            };
        }]);
        //angular.element(document).ready(function () {
        //    angular.bootstrap(document.body, ['CompanyProfileApp']);
        //});
    })();
</script>
